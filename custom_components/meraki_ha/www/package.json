{
  "name": "meraki-ha-www",
  "private": true,
  "version": "2.2.1-beta.20",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run postbuild",
    "postbuild": "cp build/meraki-panel.js . && (cp build/style.css . 2>/dev/null || true)",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "changelog": "conventional-changelog -p angular -i ../../../CHANGELOG.md -s",
    "set-version": "node ../../../get-version.js && npm version $(node ../../../get-version.js) --no-git-tag-version"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.9.3",
    "vite": "^5.2.0"
  }
}
